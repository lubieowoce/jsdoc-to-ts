{
  "compilerOptions": {
    "noEmit": true,
    "lib": ["es2023"],
    "types": ["node"],
    "module": "nodenext",
    "target": "es2022",
    "moduleResolution": "node16",
    "rewriteRelativeImportExtensions": true,

    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true
  }
}
